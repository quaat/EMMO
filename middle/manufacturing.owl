        <annotations:EMMO_967080e5_2f42_4eb2_a3a9_c58143e835f9>A manufacturing process whose product is the result of the combination of more substances.</annotations:EMMO_967080e5_2f42_4eb2_a3a9_c58143e835f9>
        <annotations:EMMO_b432d2d5_25f4_4165_99c5_5935a7763c1a>Synthesis of materials, the preparation of a cake.</annotations:EMMO_b432d2d5_25f4_4165_99c5_5935a7763c1a>
        <rdfs:label xml:lang="en">ContinuousManufacturing</rdfs:label>
    </owl:Class>
    


    <!-- http://emmo.info/emmo/middle/manufacturing#EMMO_86ca9b93_1183_4b65_81b8_c0fcd3bba5ad -->

    <owl:Class rdf:about="http://emmo.info/emmo/middle/manufacturing#EMMO_86ca9b93_1183_4b65_81b8_c0fcd3bba5ad">
        <rdfs:subClassOf>
